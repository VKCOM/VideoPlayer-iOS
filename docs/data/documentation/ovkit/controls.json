er":"doc:\/\/onevideo.OVKit\/documentation\/OVKit\/Prefetch","abstract":[{"type":"text","text":"Предзагрузка видео еще до начала воспроизведения."}]},"doc://onevideo.OVKit/documentation/OVKit/Extras":{"title":"Дополнительно","type":"topic","role":"article","url":"\/documentation\/ovkit\/extras","abstract":[{"text":"Дополнительные возможности ","type":"text"},{"code":"OVKit","type":"codeVoice"},{"text":".","type":"text"}],"identifier":"doc:\/\/onevideo.OVKit\/documentation\/OVKit\/Extras","kind":"article"},"doc://onevideo.OVKit/documentation/OVKit/PlayerControlsViewProtocol/controlMask":{"required":true,"url":"\/documentation\/ovkit\/playercontrolsviewprotocol\/controlmask","role":"symbol","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"controlMask"},{"kind":"text","text":": "},{"preciseIdentifier":"s:5OVKit11ControlMaskC","kind":"typeIdentifier","text":"ControlMask"},{"kind":"text","text":"?"}],"title":"controlMask","type":"topic","abstract":[{"text":"Актуальная на данный момент контрольная маска.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/onevideo.OVKit\/documentation\/OVKit\/PlayerControlsViewProtocol\/controlMask"},"doc://onevideo.OVKit/documentation/OVKit/PlayerControlsViewProtocol/handlePlayerTick()":{"identifier":"doc:\/\/onevideo.OVKit\/documentation\/OVKit\/PlayerControlsViewProtocol\/handlePlayerTick()","kind":"symbol","title":"handlePlayerTick()","type":"topic","role":"symbol","url":"\/documentation\/ovkit\/playercontrolsviewprotocol\/handleplayertick()","abstract":[{"type":"text","text":"Вызывается периодически во время проигрывания видео."}],"fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"handlePlayerTick","kind":"identifier"},{"text":"()","kind":"text"}],"required":true},"doc://onevideo.OVKit/documentation/OVKit/ControlMask":{"role":"symbol","kind":"symbol","url":"\/documentation\/ovkit\/controlmask","type":"topic","title":"ControlMask","identifier":"doc:\