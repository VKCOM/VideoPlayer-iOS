 {","    [super viewWillLayoutSubviews];","    ","    self.wrapped.view.frame = self.view.bounds;","}","","#pragma mark - Appearance","","- (BOOL)prefersStatusBarHidden {","    return self.wrapped.prefersStatusBarHidden;","}","","- (UIStatusBarAnimation)preferredStatusBarUpdateAnimation {","    return self.wrapped.preferredStatusBarUpdateAnimation;","}","","- (UIStatusBarStyle)preferredStatusBarStyle {","    return self.wrapped.preferredStatusBarStyle;","}","","- (BOOL)prefersHomeIndicatorAutoHidden {","    return self.wrapped.prefersHomeIndicatorAutoHidden;","}","","- (UIInterfaceOrientationMask)supportedInterfaceOrientations {","    return self.wrapped.supportedInterfaceOrientations;","}","","- (UIInterfaceOrientation)preferredInterfaceOrientationForPresentation {","    return self.wrapped.preferredInterfaceOrientationForPresentation;","}","","- (BOOL)shouldAutorotate {","    return self.wrapped.shouldAutorotate;","}","","#pragma mark - OVKFullscreenControllerProtocol","","- (OVKPlayerView *)playerView {","    return self.wrapped.playerView;","}","","- (UIView<OVKFullscreenCustomView> *)customTopView {","    return self.wrapped.customTopView;","}","","- (void)setCustomTopView:(UIView<OVKFullscreenCustomView> *)customTopView {","    self.wrapped.customTopView = customTopView;","}","","- (UIView<OVKFullscreenCustomView> *)customMiddleView {","    return self.wrapped.customMiddleView;","}","","- (void)setCustomMiddleView:(UIView<OVKFullscreenCustomView> *)customMiddleView {","    self.wrapped.customMiddleView = customMiddleView;","}","","- (UIView<OVKFullscreenCustomBottomView> *)customBottomView {","    return self.wrapped.customBottomView;","}","","- (BOOL)enableBottomControls {","    return self.wrapped.enableBottomControls;","}","","- (void)setEnableBottomControls:(BOOL)enableBottomControls {","    self.wrapped.enableBottomControls = enableBottomControls;","}","","- (BOOL)alwaysShowSoundButton {","    return self.wrapped.alwaysShowSoundButton;","}","","- (void)se